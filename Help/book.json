{
    "title": "Isogeo Online Documentation",
    "description": "Online help for Isogeo platform",
    "isbn": "",
    "links": {
        "sidebar": {
            "Site Isogeo": "http://www.isogeo.com/",
            "Application": "https://app.isogeo.com/"
        },
        "sharing": {
            "google": true,
            "facebook": false,
            "twitter": true,
            "weibo": false,
            "all": false
        }
    },
    "plugins": [
        "anchors",
        "customtheme",
        "expandable-chapters-small",
        "ga",
        "language-picker",
        "navigator",
        "sitemap",
        "splitter",
        "tags"
    ],
    "pluginsConfig": {
        "customtheme": {
            "css": [
                "_styles/custom.css"
            ]
        },
        "fontSettings": {
            "theme": "white",
            "family": "sans"
        },
        "ga": {
            "token": "UA-22345848-2"
        },
         "language-picker": {
            "grid-columns": 3
        },
        "sitemap": {
            "hostname": "http://help.isogeo.com/"
        },
        "tags": {
            "placement": "bottom"
        }

    },
    "pdf": {
        "pageNumbers": true,
        "fontSize": 12,
        "paperSize": "a4",
        "margin": {
            "right": 45,
            "left": 45,
            "top": 36,
            "bottom": 36
        },
        "headerTemplate": "_TITLE_",
        "footerTemplate": "_SECTION_"
    },
    "styles": {
        "custom": "styles/custom.css"
    },
    "variables": {
        "version": "2.15.0-23",
        "author": "Isogeo"
    }
}